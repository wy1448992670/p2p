//切换附件与通讯录列表
function tabList(i,_this){
	$('.tabColor').css('background','#E4E4E4');
	$(_this).css('background','#D7D7D7');
	$('.tabList').hide();
	$('.tabList').eq(i).show();
}

$('._xf_htgl_jgtitle').click(function(){
	$(this).next().toggle(100);
})

$(function(){
	/* 高亮显示 */
	showHighLight(2, 2, 3);
});
</script>
<script>
	// 搜索事件
	function getPage(){
			var str1 = '';
			var mobile = $('#mobile').val();
			var uid = $('#uid').val();
			 // alert(mobile);
			var layerMsg = layer.load('加载通讯录中…',{ icon: 0, shade: [0.5,'black']});
			$.get("@{supervisor.postLoanManager.UserListAction.getUserAddressList}",{id: uid,returnJson: 1,currPage: 1,mobile: mobile},function(res){
							
							$.each(res.page,function(i,e){
								str1 +='<tr><td>'+(i+1)+'</td><td>'+e.name+'</td><td class="tleft">'+e.mobile+'</td></tr>'
								
							})
							$("#pagination3").pagination({
									currentPage: 1,
									totalPage: res.totalPageCount,
									isShow: true,
									count: 7,
									homePageText: "首页",
									endPageText: "尾页",
									prevPageText: "上一页",
									nextPageText: "下一页",
									callback: function(current) {
										var str = '';
										var layerMsg = layer.load('加载通讯录中…',{ icon: 0, shade: [0.5,'black']});
										$.get("@{supervisor.postLoanManager.UserListAction.getUserAddressList}",{id: uid,returnJson: 1,currPage: current,mobile: mobile},function(res){
											$.each(res.page,function(i,e){
												console.log(e.name);
												str +='<tr><td>'+parseInt(((current-1)*10)+1+i)+'</td><td>'+e.name+'</td><td class="tleft">'+e.mobile+'</td></tr>'
												
											})
											layer.close(layerMsg); 
										$("#contacts").html('<tr><th width="80">序号</th><th width="500">联系人姓名</th><th align="left">联系电话</th></tr>'+str);
										});
										$("#current3").text(current);
										
									}
							});
							layer.close(layerMsg); 
						$("#contacts").html('<tr><th width="80">序号</th><th width="500">联系人姓名</th><th align="left">联系电话</th></tr>'+str1);
			});
		
			
	}
		// 分页js
			$(function() {
				var pageCount = ${userAddressPage.totalPageCount}||0;
				var user_id = '';
				
				if($('#contacts').find('tr').length>2){
					user_id = $('#contacts').find('tr').eq(1).attr('uid');
				}
				$('#uid').val(user_id);
				var mobile = $("#mobile").val();
				$("#pagination3").pagination({
					currentPage: 1,
					totalPage: pageCount,
					isShow: true,
					count: 7,
					homePageText: "首页",
					endPageText: "尾页",
					prevPageText: "上一页",
					nextPageText: "下一页",
					callback: function(current) {
						var str = '';
						var layerMsg = layer.load('加载通讯录中…',{ icon: 0, shade: [0.5,'black']});
						$.get("@{supervisor.postLoanManager.UserListAction.getUserAddressList}",{id: user_id,returnJson: 1,currPage: current,mobile:mobile},function(res){
							$.each(res.page,function(i,e){
								str +='<tr><td>'+parseInt(((current-1)*10)+1+i)+'</td><td>'+e.name+'</td><td class="tleft">'+e.mobile+'</td></tr>'
								
							})
							layer.close(layerMsg); 
						$("#contacts").html('<tr><th width="80">序号</th><th width="500">联系人姓名</th><th align="left">联系电话</th></tr>'+str);
						});
						$("#current3").text(current);
						
					}
				});
			 	 

				
			});
</script>
