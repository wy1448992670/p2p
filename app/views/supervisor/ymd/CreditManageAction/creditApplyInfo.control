	 	layer.close(index);
	});
}
function notThrough(){
	layer.confirm('是否确认拒绝？', function(index){
		$("#audit").attr('action','@{supervisor.ymd.CreditManageAction.notThrough()}');
		$("#audit").submit();
	 	layer.close(index);
	});
}
function freeze(){
	layer.confirm('是否确认冻结额度？', function(index){
		$("#audit").attr('action','@{supervisor.ymd.CreditManageAction.freeze()}');
		$("#audit").submit();
	 	layer.close(index);
	});
}
function unfreeze(){
	layer.confirm('是否确认解冻额度？', function(index){
		$("#audit").attr('action','@{supervisor.ymd.CreditManageAction.unfreeze()}');
		$("#audit").submit();
	 	layer.close(index);
	});
}
function closeCredit(){
	layer.confirm('是否确认关闭额度？', function(index){
		$("#audit").attr('action','@{supervisor.ymd.CreditManageAction.close()}');
